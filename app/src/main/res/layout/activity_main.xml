<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@color/colorPrimary">

    <ImageButton
        android:id="@+id/githubButton"
        android:layout_width="115dp"
        android:layout_height="122dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/github_button"
        app:layout_constraintBottom_toTopOf="@+id/linkedinButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/linkedinButton"
        android:layout_width="115dp"
        android:layout_height="122dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/linkedin_button"
        app:layout_constraintBottom_toTopOf="@+id/siteButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.902"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/githubButton" />

    <ImageButton
        android:id="@+id/siteButton"
        android:layout_width="115dp"
        android:layout_height="122dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/my_syte_button"
        app:layout_constraintBottom_toTopOf="@+id/twitterButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linkedinButton" />

    <ImageButton
        android:id="@+id/twitterButton"
        android:layout_width="115dp"
        android:layout_height="122dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/twitter_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.902"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/siteButton"
        app:layout_constraintVertical_bias="0.313" />


</androidx.constraintlayout.widget.ConstraintLayout>